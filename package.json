		"@tarojs/redux-h5": "1.3.20",
		"@tarojs/rn-runner": "1.3.20",
		"@tarojs/router": "1.3.20",
		"@tarojs/taro": "1.3.20",
		"@tarojs/taro-alipay": "1.3.20",
		"@tarojs/taro-h5": "1.3.20",
		"@tarojs/taro-swan": "1.3.20",
		"@tarojs/taro-tt": "1.3.20",
		"@tarojs/taro-weapp": "1.3.20",
		"acorn-jsx": "^5.1.0",
		"blueimp-md5": "^2.10.0",
		"classnames": "latest",
		"dateformat": "^3.0.3",
		"file-loader": "^4.0.0",
		"jsonwebtoken": "^8.5.1",
		"jws": "^3.2.2",
		"jwt-decode": "^2.2.0",
		"nerv-devtools": "^1.5.1",
		"nervjs": "^1.5.1",
		"redux": "^4.0.0",
